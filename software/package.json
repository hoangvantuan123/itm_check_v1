{
    "name": "itm",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
        "start": "electron main.js",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "com.example.myelectronapp",
        "mac": {
            "target": [
                "dmg",
                "zip"
            ],
            "icon": "build/icon.icns"
        },
        "win": {
            "target": ["nsis", "zip"],
            "icon": "build/icon.ico"
        },
        "dmg": {
            "contents": [{
                    "x": 410,
                    "y": 150,
                    "type": "link",
                    "path": "/Applications"
                },
                {
                    "x": 130,
                    "y": 150,
                    "type": "file"
                }
            ]
        }
    },
    "devDependencies": {
        "electron": "^29.0.1",
        "electron-builder": "^24.12.0",
        "electron-packager": "^17.1.2"
    }
}